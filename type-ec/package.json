    "@vue/test-utils": "^1.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^27.0.5",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "ts-jest": "^27.0.3",
